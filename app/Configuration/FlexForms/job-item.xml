<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
    <meta type="array">
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <general>
            <ROOT type="array">
                <TCEforms>
                    <sheetTitle>Job Optionen</sheetTitle>
                </TCEforms>
                <el type="array">
                    <testtool>
                        <TCEforms type="array">
                            <label>Google Jobs Anzeige Testen</label>
                            <config type="array">
                                <type>none</type>
                                <default>
                                    <![CDATA[https://search.google.com/structured-data/testing-tool/u/0/]]></default>
                            </config>
                        </TCEforms>
                    </testtool>

                    <filter type="array">
                        <TCEforms type="array">
                            <label>Arbeiten</label>
                            <config type="array">
                                <type>select</type>
                                <renderType>selectSingle</renderType>
                                <renderType>selectMultipleSideBySide</renderType>
                                <items>
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">
                                            LLL:/fileadmin/schroeder/Resources/Private/Language/locallang.xlf:teilzeit
                                        </numIndex>
                                        <numIndex index="1">teilzeit</numIndex>
                                    </numIndex>
                                    <numIndex index="1" type="array">
                                        <numIndex index="0">
                                            LLL:/fileadmin/schroeder/Resources/Private/Language/locallang.xlf:vollzeit
                                        </numIndex>
                                        <numIndex index="1">vollzeit</numIndex>
                                    </numIndex>
                                </items>
                                <enableMultiSelectFilterTextfield>1</enableMultiSelectFilterTextfield>
                            </config>
                        </TCEforms>
                    </filter>
                    <skills type="array">
                        <TCEforms type="array">
                            <label>Skills (Komma getrennt)</label>
                            <config type="array">
                                <type>input</type>
                                <size>255</size>
                                <eval>trim</eval>
                                <default></default>
                            </config>
                        </TCEforms>
                    </skills>

                    <google_desc type="array">
                        <TCEforms type="array">
                            <label>Beschreibung</label>
                            <config>
                                <type>text</type>
                                <cols>24</cols>
                                <rows>3</rows>
                                <enableRichtext>1</enableRichtext>
                            </config>
                            <defaultExtras>richtext[*]:rte_transform[mode=ts_css]</defaultExtras>
                        </TCEforms>
                    </google_desc>

                </el>
            </ROOT>
        </general>
        <google_jobs>
            <ROOT type="array">
                <TCEforms>
                    <sheetTitle>Firma</sheetTitle>
                </TCEforms>
                <el type="array">
                    <hiring_name type="array">
                        <TCEforms type="array">
                            <label>Name</label>
                            <description>hiringOrganization</description>
                            <config type="array">
                                <type>input</type>
                                <size>255</size>
                                <eval>trim</eval>
                                <default><![CDATA[Küchen-Studio ARDLAND GmbH]]></default>
                            </config>
                        </TCEforms>
                    </hiring_name>

                    <hiring_logo type="array">
                        <TCEforms type="array">
                            <label>Logo</label>
                            <description>(hiringOrganization) Standard Logo Überschreiben</description>
                            <config type="array">
                                <type>inline</type>
                                <maxitems>1</maxitems>

                                <foreign_table>sys_file_reference</foreign_table>
                                <foreign_field>uid_foreign</foreign_field>
                                <foreign_sortby>sorting_foreign</foreign_sortby>
                                <foreign_table_field>tablenames</foreign_table_field>
                                <foreign_match_fields>
                                    <fieldname>image</fieldname>
                                </foreign_match_fields>
                                <foreign_label>uid_local</foreign_label>
                                <foreign_selector>uid_local</foreign_selector>
                                <overrideChildTca>
                                    <columns>
                                        <uid_local>
                                            <config>
                                                <appearance>
                                                    <elementBrowserType>file</elementBrowserType>
                                                    <elementBrowserAllowed></elementBrowserAllowed>
                                                </appearance>
                                            </config>
                                        </uid_local>
                                    </columns>
                                    <types>
                                        <types index="0">
                                            <showitem>--palette--;LLL:EXT:lang/Resources/Private/Language/locallang_tca.xlf:sys_file_reference.imageoverlayPalette;imageoverlayPalette,
                                                --palette--;;filePalette</showitem>
                                        </types>
                                        <types index="1">
                                            <showitem>--palette--;LLL:EXT:lang/Resources/Private/Language/locallang_tca.xlf:sys_file_reference.imageoverlayPalette;imageoverlayPalette,
                                                --palette--;;filePalette</showitem>
                                        </types>
                                        <types index="2">
                                            <showitem>--palette--;LLL:EXT:lang/Resources/Private/Language/locallang_tca.xlf:sys_file_reference.imageoverlayPalette;imageoverlayPalette,
                                                --palette--;;filePalette</showitem>
                                        </types>
                                        <types index="3">
                                            <showitem>--palette--;LLL:EXT:lang/Resources/Private/Language/locallang_tca.xlf:sys_file_reference.audioOverlayPalette;audioOverlayPalette,
                                                --palette--;;filePalette</showitem>
                                        </types>
                                        <types index="4">
                                            <showitem>--palette--;LLL:EXT:lang/Resources/Private/Language/locallang_tca.xlf:sys_file_reference.videoOverlayPalette;videoOverlayPalette,
                                                --palette--;;filePalette</showitem>
                                        </types>
                                        <types index="5">
                                            <showitem>--palette--;LLL:EXT:lang/Resources/Private/Language/locallang_tca.xlf:sys_file_reference.imageoverlayPalette;imageoverlayPalette,
                                                --palette--;;filePalette</showitem>
                                        </types>
                                    </types>
                                </overrideChildTca>
                                <filter>
                                    <userFunc>TYPO3\CMS\Core\Resource\Filter\FileExtensionFilter->filterInlineChildren</userFunc>
                                    <parameters>
                                        <allowedFileExtensions></allowedFileExtensions>
                                        <disallowedFileExtensions></disallowedFileExtensions>
                                    </parameters>
                                </filter>
                                <appearance>
                                    <useSortable>1</useSortable>
                                    <headerThumbnail>
                                        <field>uid_local</field>
                                        <width>45</width>
                                        <height>45c</height>
                                    </headerThumbnail>
                                    <enabledControls>
                                        <info>0</info>
                                        <new>0</new>
                                        <dragdrop>0</dragdrop>
                                        <sort>0</sort>
                                        <hide>0</hide>
                                        <delete>1</delete>
                                    </enabledControls>
                                </appearance>
                            </config>
                        </TCEforms>
                    </hiring_logo>

                    <location_street type="array">
                        <TCEforms type="array">
                            <label>Straße</label>
                            <description>jobLocation</description>
                            <config type="array">
                                <type>input</type>
                                <size>255</size>
                                <eval>trim</eval>
                                <default>Bosinks Kamp 2</default>
                            </config>
                        </TCEforms>
                    </location_street>
                    <location_city type="array">
                        <TCEforms type="array">
                            <label>Stadt</label>
                            <description>jobLocation</description>
                            <config type="array">
                                <type>input</type>
                                <size>255</size>
                                <eval>trim</eval>
                                <default>Nordhorn</default>
                            </config>
                        </TCEforms>
                    </location_city>
                    <location_zip type="array">
                        <TCEforms type="array">
                            <label>PLZ</label>
                            <description>jobLocation</description>
                            <config type="array">
                                <type>input</type>
                                <size>255</size>
                                <eval>trim</eval>
                                <default>48531</default>
                            </config>
                        </TCEforms>
                    </location_zip>
                    <location_country type="array">
                        <TCEforms type="array">
                            <label>Land</label>
                            <description>jobLocation</description>
                            <config type="array">
                                <type>input</type>
                                <size>255</size>
                                <eval>trim</eval>
                                <default>DE</default>
                            </config>
                        </TCEforms>
                    </location_country>
                </el>
            </ROOT>
        </google_jobs>
    </sheets>
</T3DataStructure>
